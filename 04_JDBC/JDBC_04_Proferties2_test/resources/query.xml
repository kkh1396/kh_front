<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>query.....</comment>
	<entry key="insertMember">
		INSERT INTO MEMBER " + "VALUES (SEQ_MNO.NEXTVAL, ?, ?, ?, ?, ? ,? ,? ,?,
		SYSDATE)
	</entry>
	<entry key="selectAll"> select * from member order by Memberid </entry>
	<entry key="searchById"> select * from member where MEMBERID like '%?%' </entry>
	<entry key="updateMember"> update member set MEMBERPW = ? ,
		GENDER = ? , ADDRESS = ? , HOBBY = ? where MEMBERID = ?
	</entry>
	<entry key="deleteMember"> delete from member where memberid = ?
	</entry>

</properties>
